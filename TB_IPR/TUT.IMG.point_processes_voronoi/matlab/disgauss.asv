function [X,Y] = disgauss(nbi,nbp)
%  calcule et affiche les paramètres de nbi distributions gausienne centrée en (mx,my)et d'écart type sigma, sachant que les points hors [0,10] sont supprimés. 
for i =1:nbi
 %X=normrnd(mx,sigma,nbp,1);
 %Y=normrnd(my,sigma,nbp,1);
 X=0 + sqrt(1.41) *randn(1,nbp);
 Y=0 + sqrt(2) *randn(1,nbp);
 % on ne garde que les point dans l'image
 X1=(-2<X<2);
 X=X1.*X;
 Y1=(-2<Y<2);
 Y=Y1.*Y;
  parametersxy(X,Y,2);
  %test1(X,Y);
end
end